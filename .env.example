# Environment variables for local development
# Copy this file to .env and fill in your values

# Azure Service Principal credentials
AZURE_CLIENT_ID=your-client-id
AZURE_CLIENT_SECRET=your-client-secret
AZURE_TENANT_ID=your-tenant-id

# Microsoft Fabric workspace IDs
FABRIC_WORKSPACE_ID_DEV=your-dev-workspace-id
FABRIC_WORKSPACE_ID_STAGING=your-staging-workspace-id
FABRIC_WORKSPACE_ID_PROD=your-prod-workspace-id

# Optional: Custom log level
LOG_LEVEL=INFO
